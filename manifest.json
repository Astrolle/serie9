{
  "manifest_version": 3,
  "name": "Serie9",
  "version": "1.0.0",
  "description": "Una app para enfocarte, detenerte y respirar.",
  "icons": {
    "192": "icon-192.png",
    "512": "icon-512.png"
  },
  "action": {
    "default_title": "Serie9",
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "service-worker.js"
  }
}
